1. git init
2. git add section1.1 section3.1 section5.1
3. git commit -m "Added 3 files containing sections 1.1, 3.1 and 5.1"
4. git rm section3.1
5. git reset HEAD section3.1
6. git checkout section3.1
7. git remote add origin https://github.com/MostafaShehab/sprint0
8. git push -u origin master
9. git branch ReadMe
10. git checkout ReadMe
11. git add README.md
12.  git commit -m "Added a readme.md file containing details on the repo"
13. git checkout master
14. git merge ReadMe
15. git add AllMyCommands
16. git commit -m "Added AllMyCommands file which contains all commands used to create the repo"
17. git push -u origin master

